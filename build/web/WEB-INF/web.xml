<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>buy</servlet-name>
        <servlet-class>buy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>havlchk</servlet-name>
        <servlet-class>havlchk</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>avlins</servlet-name>
        <servlet-class>avlins</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>client.update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update2</servlet-name>
        <servlet-class>update2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adlogin</servlet-name>
        <servlet-class>adlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatecli</servlet-name>
        <servlet-class>Admin.updatecli</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateusr</servlet-name>
        <servlet-class>Admin.updateusr</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>buy</servlet-name>
        <url-pattern>/buy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>havlchk</servlet-name>
        <url-pattern>/havlchk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>avlins</servlet-name>
        <url-pattern>/avlins</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update2</servlet-name>
        <url-pattern>/client/update2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adlogin</servlet-name>
        <url-pattern>/adlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatecli</servlet-name>
        <url-pattern>/Admin/updatecli</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateusr</servlet-name>
        <url-pattern>/Admin/updateusr</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
